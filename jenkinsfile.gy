pipeline{

	agent any

	environment {
		DOCKERHUB_CREDENTIALS=credentials('dockercred')
	}

	stages {
	    
	    stage('build') {
	        steps {
	            sh 'git clone https://github.com/noobx7wt2fDL/wordpress-slproj2.git'
	            sh 'cd wordpress-slproj2 && ls && docker-compose up -d'
	        }
	    }

		stage('verify') {
			steps {
				sh 'docker image ls'
                                sh 'docker ps -a'
                                sh 'docker volume ls'
			}
		}
				
	}

}

